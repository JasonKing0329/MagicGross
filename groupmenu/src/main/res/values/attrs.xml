<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GroupMenuVer">
        <attr name="menu" />
        <attr name="verItemMargin" format="dimension" />
        <attr name="verItemSize" format="dimension" />
        <attr name="verItemBackgroundColor" format="color" />
        <attr name="verItemPadding" format="dimension" />
    </declare-styleable>

    <!--从MenuItem复制部分-->
    <declare-styleable name="GMMenuItem">
        <attr name="android:id"/>
        <attr name="android:title"/>
        <attr name="android:icon"/>
        <attr name="android:checkable"/>
        <attr name="android:checked"/>
        <attr name="android:visible"/>
        <attr name="android:enabled"/>
        <attr name="android:onClick"/>
        <attr name="showAsAction"/>
    </declare-styleable>

</resources>