<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--加载中对话框-->
    <declare-styleable name="CircularProgress">
        <attr name="borderWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LineChart">
        <attr name="minXCellWidth" format="dimension" />
        <attr name="normalXCellWidth" format="dimension" />
        <attr name="valueTextColor" format="color" />
        <attr name="valueTextSize" format="dimension" />
        <attr name="lineColor" format="color" />
    </declare-styleable>

    <declare-styleable name="AxisChart">
        <attr name="axisXExtendLineWidth" format="dimension" />
        <attr name="axisXTextSize" format="dimension" />
        <attr name="axisYTextSize" format="dimension" />
        <attr name="axisYTextWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="BarChart">
        <attr name="barWidth" format="dimension" />
        <attr name="barGap" format="dimension" />
    </declare-styleable>

    <declare-styleable name="WrapLayout">
        <attr name="wlyHorizontalSpacing" format="dimension" />
        <attr name="wlyVerticalSpacing" format="dimension" />
        <attr name="wlyGravity" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
            <enum name="bottom" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="WrapLayout_Layout">
        <attr name="wlyLayout_gravity" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
            <enum name="bottom" value="2" />
        </attr>
    </declare-styleable>
</resources>